    },
    "eslint": {
      "optional": true
    },
    "pinia": {
      "optional": true
    },
    "workbox-build": {
      "optional": true
    }
  },
  "engines": {
    "node": "^30 || ^28 || ^26 || ^24 || ^22 || ^20",
    "npm": ">= 6.14.12",
    "yarn": ">= 1.17.3"
  },
  "volta": {
    "node": "lts"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --cache --fix"
  }
}